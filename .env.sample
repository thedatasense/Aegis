# Neon/Postgres
DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DBNAME

# Strava OAuth (from https://www.strava.com/settings/api)
STRAVA_CLIENT_ID=12345
STRAVA_CLIENT_SECRET=your_secret
STRAVA_REFRESH_TOKEN=your_refresh_token

# Optional: run periodic Strava sync every N minutes
# STRAVA_SYNC_INTERVAL_MINUTES=60

# TickTick Open API (https://developer.ticktick.com/)
TICKTICK_CLIENT_ID=your_client_id
TICKTICK_CLIENT_SECRET=your_client_secret
TICKTICK_REFRESH_TOKEN=your_refresh_token
